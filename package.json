{
  "author": "Jared Allard <jaredallard@outlook.com>",
  "name": "jsreport-nexe",
  "description": "create single executables out of your [node/io].js applications",
  "license": "MIT",
  "version": "1.3.1",
  "contributors": [
    {
      "name": "Criag Condon",
      "email": "craig.j.condon@gmail.com",
      "url": "http://crcn.io/"
    }
  ],
  "scripts": {
    "test": "echo node: $(node -v) && node_modules/.bin/mocha --reporter mocha-circleci-reporter test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jaredallard/nexe.git"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "async": "2.1.4",
    "browserify": "14.0.0",
    "colors": "1.1.2",
    "fs-extra": "2.0.0",
    "glob": "7.0.0",
    "gunzip-maybe": "1.3.1",
    "insert-module-globals": "7.0.1",
    "mkdirp": "0.5.1",
    "module-deps": "4.0.5",    
    "progress": "1.1.8",
    "request": "2.67.0",
    "tar-stream": "1.3.1",
    "through2": "2.0.3",
    "yargs": "6.6.0"
  },
  "nexe": {
    "input": "./bin/nexe",
    "output": "nexe^$",
    "temp": "src",
    "runtime": {
      "framework": "nodejs",
      "version": "5.5.0",
      "ignoreFlags": true
    }
  },
  "preferGlobal": true,
  "bin": {
    "nexe": "bin/nexe"
  },
  "devDependencies": {
    "mocha": "3.2.0",
    "mocha-circleci-reporter": "0.0.2"
  }
}
